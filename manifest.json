{
    "name": "ここすきボタン",
    "version": "0.23",
    "manifest_version": 2,
    "permissions": [
        "tabs",
        "storage",
        "<all_urls>",
        "input"
        ], 
    "content_scripts": [
        {
            "matches": ["https://www.youtube.com/*"],
            "css": ["style.css"],
            "js": ["script/jquery-3.5.1.min.js","script/make-stamp.js"],
            "all_frames": true
        }
    ],
    "externally_connectable": {
        "matches": ["https://www.youtube.com/*"]
      },
    "icons": {
        "48": "icon/48.png"
    },
    "browser_action": {
        "default_icon": "icon/48.png",
        "default_title": "titile",
         "default_popup": "popup.html"
    }
}